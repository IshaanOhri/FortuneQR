<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sarveshsbibhuty.fortuneqr">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/camerabutton"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/camerabutton"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name=".team_code"
            android:screenOrientation="portrait"/>
        <activity android:name=".team_reg"
            android:screenOrientation="portrait" />
        <activity android:name=".qrscanner"
            android:screenOrientation="portrait" />
        <activity android:name=".splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".endgame"
            android:screenOrientation="portrait"/>
        <activity android:name=".aboutus"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>